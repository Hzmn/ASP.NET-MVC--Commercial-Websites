    <script type="text/javascript">
        $(document).ready(function () {
            $(".floor").click(function () {
                if ($(this).hasClass("go-top")) {
                    $("body").animate({ scrollTop: 0 })
                }
                else {
                    $(this).addClass("flover").siblings().removeClass("flover");
                    var a = $(this).attr("floor_num");
                    var b = $("div[floor_num=" + a + "]").offset().top;
                    $("body").animate({ scrollTop: b });
                }
            })

            $(".close").click(function () {
                $("#remo").remove()
            });

            $(".jqhover").mouseover(function () {
                $(this).find("div.sub_hidden").css("display", "block");
                $(this).addClass("w_3_hov");
                $(this).find("span.spacer").css("display", "none");
                $(this).find("a").css("color", "#863177");
                $(this).find("div.icon").css("margin-left", "10px");
                var t = $(this).find("div.icon").css("backgroundPosition");
                var position = t.split(" ");
                var x = parseInt(position[0]) - 30 + "px ";
                var y = position[1];
                $(this).find("div.icon").css({ "backgroundPosition": x + y, "backgroundColor": "white" });
                var b = $("#w_3_c_l").offset().top;
                var a = $(this).find("div.sub_hidden").offset().top;
                var c = $(this).find("div.sub_hidden").height();
                if (a + c >= b + 442) {
                    $(this).find("div.sub_hidden").css("top", -(a + c - b - 442 + 1));
                }
            })
            $(".jqhover").mouseout(function () {
                $(this).find("div.sub_hidden").css("display", "none");
                $(this).removeClass("w_3_hov");
                $(this).find("span.spacer").css("display", "block");
                $(this).find("a").css("color", "white");
                var t = $(this).find("div.icon").css("backgroundPosition");
                var position = t.split(" ");
                var x = parseInt(position[0]) + 30 + "px ";
                var y = position[1];
                $(this).find("div.icon").css({ "backgroundPosition": x + y, "backgroundColor": "#863177" });
            })
        })


    </script>
